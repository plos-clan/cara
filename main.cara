const fib_num = 10i64;

const printf = proto C[printf] fn(format: *i8, a: i64) -> i32;

const print_num = fn() {
    printf("%d\n", fib_num);
};

const main = extern C[main] fn() -> i64 {
    let mut return_value = 0;
    for a in (0,11) {
        return_value = a + return_value;
    };
    print_num(return_value);
    
    0
};

const fib = fn(n: i64) -> i64 {
    if n <= 1i64 {
        n
    } else {
        fib(n - 1i64) + fib(n - 2i64)
    }
};
